@{
    Layout = "_LayoutExterno";
}



<div class="col-lg-3 col-12"></div>
<div class="col-lg-6 col-12">

    <div class="col-lg-12 col-12 text-center">
        <h3 class="mb-4 pb-2">Inicio de Sesión</h3>
    </div>

    <form action="#" method="post" class="custom-form contact-form" role="form">
        <div class="row">
            <div class="col-lg-12 col-12">
                <div class="form-floating">
                    <input type="text" name="name" id="name" class="form-control" placeholder="Name" required="">
                    <label for="floatingInput">Identificación</label>
                </div>
            </div>

            <div class="col-lg-12 col-12">
                <div class="form-floating">
                    <input type="password" name="subject" id="name" class="form-control" placeholder="Name" required="">
                    <label for="floatingInput">Contraseña</label>
                </div>
            </div>

            <div class="col-lg-5 col-12">
                <button type="submit" class="form-control">Procesar</button>
            </div>

        </div>
    </form>
</div>